    <script type="text/javascript">
        function ChangeMenuSize(expand) {
            if (expand)
                parent.document.getElementById('frameHome').cols = "*,180";
            else
                parent.document.getElementById('frameHome').cols = "*,10";
        }

        function GoFavourites(select) {
            var url = select.options[select.selectedIndex].value;
            if (url != "") {
                top.mainFrame.location.href = url;
            }
        }
        
        function load()
        {
            var elements = document.getElementsByTagName('a');
            for(i=0;i<elements.length;i++){
                if(elements[i].href.indexOf("javascript:")==-1){
                    if(elements[i].href.indexOf("?")!=-1){
                        elements[i].href = elements[i].href + "&SecurityTokenTester=true";
                    }else{
                        elements[i].href = elements[i].href + "?SecurityTokenTester=true";
                    }
                }
            }
        }
        
    </script>
