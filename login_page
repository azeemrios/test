    <script type="text/javascript">
        var marqueewidth = '150px';
        var marqueeheight = '110px';
        var marqueespeed = 1;
        var initPause = 1000;
        var full = 1;
        var pauseit = 1;
        var iebreak = '';
        var marqueecontent = document.getElementById("TableDiv").innerHTML;
        var copyspeed = marqueespeed;
        var pausespeed = (pauseit == 0) ? copyspeed : 0;
        var iedom = document.all || document.getElementById;
        var actualheight = '';
        var cross_marquee, cross_marquee2;
        var cantAlertas = 0;
        var maxAlertasRow = 1;

        function PrepareMarquee() {
            //document.getElementById("tableSinAlertas").disabled = true;

            with (document) {
                marquee = '<div style="position:relative;width:' + marqueewidth + ';height:' + marqueeheight + ';overflow:hidden" onMouseover="copyspeed=pausespeed" onMouseout="copyspeed=marqueespeed">';
                marquee += '<div id="iemarquee" style="position:absolute;left:0px;top:0px;width:100%;"></div>';
                marquee += '<div id="iemarquee2" style="position:absolute;left:0px;top:0px;width:100%;z-index:100;background:white;"></div>';
                marquee += '</div>';

                cantAlertas = parseFloat(document.getElementById("cantidadAlertas").value);
                if (cantAlertas > maxAlertasRow) {
                    marqueecontent = document.getElementById("TableDiv").innerHTML;
                    document.getElementById("TableDiv").innerHTML = marquee;

                }
                else if (cantAlertas == 0) {
                    document.getElementById("SinAlertasCell").style.display = '';
                    document.getElementById("TableDiv").style.display = 'none';
                    //document.getElementById("Alerts1_LabelSinAlertas").disabled = false;


                }
            }
        }

        function Populate() {
            if (cantAlertas > maxAlertasRow) {
                var lb = document.getElementById && !document.all ? '' : iebreak;
                cross_marquee = document.getElementById ? document.getElementById("iemarquee") : document.all.iemarquee;
                cross_marquee2 = document.getElementById ? document.getElementById("iemarquee2") : document.all.iemarquee2;
                cross_marquee2.innerHTML = cross_marquee.innerHTML = marqueecontent + lb;
                actualheight = cross_marquee.offsetHeight;
                cross_marquee2.style.top = (parseInt(cross_marquee.style.top) + actualheight) + "px";

                setTimeout('lefttime = setInterval("ScrollMarquee()", 40)', initPause);
            }
        }

        function ScrollMarquee() {
            if (parseInt(cross_marquee.style.top) < (actualheight * (-1)))
                cross_marquee.style.top = (parseInt(cross_marquee2.style.top) + actualheight) + "px";
            if (parseInt(cross_marquee2.style.top) < (actualheight * (-1)))
                cross_marquee2.style.top = (parseInt(cross_marquee.style.top) + actualheight) + "px";

            cross_marquee2.style.top = parseInt(cross_marquee2.style.top) - copyspeed + "px";
            cross_marquee.style.top = parseInt(cross_marquee.style.top) - copyspeed + "px";
        }

    
    </script>


